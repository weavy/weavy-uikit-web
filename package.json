{
  "name": "@weavy/uikit-web",
  "version": "27.0.0",
  "author": "Weavy",
  "description": "Web components UI-kit for Weavy",
  "homepage": "https://github.com/weavy/weavy-uikit-web",
  "license": "MIT",
  "type": "module",
  "files": [
    "cli",
    "dist",
    "lib",
    "locales",
    "test",
    "utils",
    "xliff",
    "*.json",
    "*.md"
  ],
  "main": "./dist/build/weavy.js",
  "module": "./dist/build/weavy.esm.bundle.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/build/weavy.esm.js",
      "require": "./dist/build/weavy.js"
    },
    "./dist/weavy.js": {
      "import": "./dist/build/weavy.esm.bundle.js",
      "require": "./dist/build/weavy.js",
      "default": "./dist/build/weavy.js"
    },
    "./dist/*": "./dist/*",
    "./lib/*": "./lib/*",
    "./utils/*": "./utils/*"
  },
  "customElements": "dist/custom-elements.json",
  "scripts": {
    "clean": "rimraf dist/**/* --glob",
    "prepack": "run-s clean build",
    "prebuild": "run-p types localize-build",
    "build": "run-s build:*",
    "build:esm": "vite build --config dev/vite.config.js",
    "build:legacy": "vite build --config dev/vite.config.legacy.js",
    "watch": "vite build --config dev/vite.config.js --watch",
    "start": "vite --config dev/vite.config.js",
    "localize-build": "lit-localize build --config=dev/lit-localize.json",
    "localize-extract": "lit-localize extract --config=dev/lit-localize.json",
    "docs": "wca analyze lib --format markdown --outDir dist/docs",
    "lint": "eslint lib",
    "types": "tsc",
    "analyze": "lit-analyzer lib/**/*",
    "pretest": "run-p lint analyze types",
    "test": "run-s test:*",
    "test:unit": "web-test-runner --config dev/web-test-runner.config.mjs --playwright --browsers chromium firefox webkit --concurrency 1 --concurrent-browsers 1",
    "test:unit:watch": "web-test-runner --config dev/web-test-runner.config.mjs --watch --debug",
    "test-disabled:e2e": "playwright test --reporter=list --quiet"
  },
  "bin": {
    "weavy": "./cli/weavy-cli.mjs"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/language": "^6.11.0",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.4",
    "@floating-ui/dom": "^1.6.13",
    "@lit/context": "^1.1.4",
    "@lit/localize": "^0.12.2",
    "@material/material-color-utilities": "^0.2.7",
    "@mdi/js": "^7.4.47",
    "@microsoft/signalr": "^8.0.0",
    "@tanstack/query-core": "^5.69.0",
    "@tanstack/query-persist-client-core": "^5.69.0",
    "@tanstack/query-sync-storage-persister": "^5.69.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "lit": "^3.2.1",
    "lodash.throttle": "^4.1.1",
    "pdfjs-dist": "^4.10.38",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@jackolope/lit-analyzer": "^3.1.0",
    "@jackolope/ts-lit-plugin": "^3.1.0",
    "@lit/localize-tools": "^0.8.0",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "^1.51.1",
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.2",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/query-devtools": "^5.67.2",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.12",
    "@types/uuid": "^10.0.0",
    "@web/test-runner": "^0.20.0",
    "@web/test-runner-playwright": "^0.11.0",
    "eslint": "^9.23.0",
    "eslint-plugin-lit": "^2.0.0",
    "eslint-plugin-lit-a11y": "^4.1.3",
    "eslint-plugin-wc": "^3.0.0",
    "globals": "^16.0.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.0",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "vite-plugin-cem": "^0.8.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-lit-css": "^2.0.0",
    "vite-plugin-static-copy": "^2.3.0",
    "web-component-analyzer": "^2.0.0"
  },
  "overrides": {
    "vite": "$vite"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  }
}
