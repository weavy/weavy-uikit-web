{
  "name": "@weavy/uikit-web",
  "version": "30.3.0",
  "author": "Weavy",
  "description": "Web components UI-kit for Weavy",
  "homepage": "https://github.com/weavy/weavy-uikit-web",
  "license": "MIT",
  "type": "module",
  "files": [
    "cli",
    "dist",
    "lib",
    "locales",
    "test",
    "utils",
    "xliff",
    "*.json",
    "*.md"
  ],
  "main": "./dist/build/weavy.js",
  "module": "./dist/build/weavy.esm.bundle.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/build/weavy.esm.js",
      "require": "./dist/build/weavy.js"
    },
    "./dist/weavy.js": {
      "import": "./dist/build/weavy.esm.bundle.js",
      "require": "./dist/build/weavy.js",
      "default": "./dist/build/weavy.js"
    },
    "./dist/*": "./dist/*",
    "./lib/*": "./lib/*",
    "./utils/*": "./utils/*"
  },
  "customElements": "dist/custom-elements.json",
  "scripts": {
    "clean": "rimraf dist/**/* --glob",
    "prepack": "run-s clean build",
    "prebuild": "run-p types localize-build",
    "build": "run-s build:*",
    "build:esm": "vite build --config dev/vite.config.js",
    "build:legacy": "vite build --config dev/vite.config.legacy.js",
    "postbuild:esm": "run-s docs",
    "watch": "vite build --config dev/vite.config.js --watch",
    "start": "vite --config dev/vite.config.js",
    "localize-build": "lit-localize build --config=dev/lit-localize.json",
    "localize-extract": "lit-localize extract --config=dev/lit-localize.json",
    "docs": "node utils/cem-to-md.js",
    "lint": "eslint lib/",
    "types": "tsc",
    "analyze": "lit-analyzer lib/**/* --strict",
    "pretest": "run-p lint analyze types",
    "test": "run-s test:*",
    "test:unit": "web-test-runner --config dev/web-test-runner.config.mjs --playwright --browsers chromium firefox webkit --concurrency 1 --concurrent-browsers 1",
    "test:unit:watch": "web-test-runner --config dev/web-test-runner.config.mjs --watch --debug",
    "test-disabled:e2e": "playwright test --reporter=list --quiet"
  },
  "bin": {
    "weavy": "./cli/weavy-cli.mjs"
  },
  "dependencies": {
    "@codemirror/autocomplete": "~6.20.0",
    "@codemirror/commands": "~6.10.1",
    "@codemirror/lang-markdown": "~6.5.0",
    "@codemirror/language": "~6.12.1",
    "@codemirror/language-data": "~6.5.2",
    "@codemirror/state": "~6.5.4",
    "@codemirror/view": "~6.39.11",
    "@floating-ui/dom": "^1.7.4",
    "@lit/context": "^1.1.6",
    "@lit/localize": "^0.12.2",
    "@material/material-color-utilities": "~0.4.0",
    "@mdi/js": "^7.4.47",
    "@microsoft/signalr": "^9.0.6",
    "@tanstack/query-core": "~5.90.19",
    "@tanstack/query-persist-client-core": "~5.91.18",
    "@tanstack/query-sync-storage-persister": "~5.90.21",
    "cors": "~2.8.6",
    "dotenv": "^17.2.3",
    "express": "~5.2.1",
    "express-session": "~1.19.0",
    "lit": "~3.3.2",
    "lodash.throttle": "^4.1.1",
    "pdfjs-dist": "^4.10.38",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/to-markdown": "github:mindroute/to-markdown#d72810c",
    "@eslint/compat": "~2.0.1",
    "@eslint/js": "^9.35.0",
    "@jackolope/lit-analyzer": "^3.2.1",
    "@jackolope/ts-lit-plugin": "^3.1.6",
    "@jackolope/web-component-analyzer": "^4.0.4",
    "@lit/localize-tools": "~0.8.1",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "~1.57.0",
    "@remcovaes/web-test-runner-vite-plugin": "^1.4.0",
    "@tanstack/eslint-plugin-query": "~5.91.3",
    "@tanstack/query-devtools": "~5.92.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.19.7",
    "@types/uuid": "^10.0.0",
    "@web/test-runner": "^0.20.2",
    "@web/test-runner-playwright": "^0.11.1",
    "cem-plugin-jsdoc-example": "~0.0.9",
    "eslint": "~9.39.2",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.1",
    "eslint-plugin-wc": "^3.0.2",
    "globals": "~17.1.0",
    "mdast-builder": "~1.1.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "~6.1.2",
    "sass": "~1.97.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "~8.53.1",
    "vite": "~7.1.12",
    "vite-plugin-cem": "^0.8.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lit-css": "^2.2.2",
    "vite-plugin-static-copy": "~3.1.6"
  },
  "overrides": {
    "vite": "$vite",
    "@custom-elements-manifest/analyzer": "0.10.10"
  },
  "engines": {
    "node": "^20.19.0 || >=22.13.0 || >=24",
    "npm": ">=10.8.2"
  }
}
